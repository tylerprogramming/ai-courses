# Agno Framework Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key (if using Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# PostgreSQL Database URL (for vector database, optional)
# DATABASE_URL=postgresql://user:password@localhost:5432/agno_db

# Agno Telemetry (set to false to disable telemetry)
# AGNO_TELEMETRY=false
